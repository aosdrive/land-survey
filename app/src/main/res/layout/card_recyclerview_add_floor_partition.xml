<!--                            android:imeOptions="actionNext"-->
<!--                            android:inputType="textCapWords"-->
<!--                            android:maxLength="100"-->
<!--                            android:padding="2dp"-->
<!--                            android:textColor="@color/Black"-->
<!--                            android:textSize="@dimen/edittext_input_size" />-->

                        <AutoCompleteTextView
                            android:id="@+id/et_commercial_activity_type"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_margin="5dp"
                            android:background="@android:color/transparent"
                            android:digits="qwertyuioplkjhgfdsazxcvbnmQWERTYUIOPLKJHGFDSAZXCVBNM .-_"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:imeOptions="actionNext"
                            android:inputType="textCapWords"
                            android:maxLength="100"
                            android:padding="2dp"
                            android:textColor="@color/Black"
                            android:textSize="@dimen/edittext_input_size" />

<!--                        <ImageView-->
<!--                            android:id="@+id/img_reset_commercial_activity"-->
<!--                            android:layout_width="wrap_content"-->
<!--                            android:layout_height="match_parent"-->
<!--                            android:src="@drawable/baseline_close_24"-->
<!--                            android:visibility="gone"-->
<!--                            app:layout_constraintEnd_toEndOf="parent"-->
<!--                            app:layout_constraintTop_toTopOf="parent" />-->

                    </LinearLayout>
                </TableRow>

                <TableRow android:layout_marginTop="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:text="@string/asterisk"
                        android:textColor="@color/textCompulsoryColor"
                        android:textSize="@dimen/edittext_title_size" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/occupancy"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/edittext_title_size" />
                </TableRow>

                <TableRow android:layout_marginBottom="@dimen/margin_5dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginVertical="@dimen/margin_4dp"
                        android:layout_weight="1"
                        android:background="@drawable/textfield_area_bg"
                        android:padding="@dimen/margin_4dp">

                        <RadioGroup
                            android:id="@+id/rg_occupancy_status"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RadioButton
                                android:layout_width="match_parent"
                                android:layout_height="25dp"
                                android:layout_weight="1"
                                android:text="@string/self" />

                            <RadioButton
                                android:layout_width="match_parent"
                                android:layout_height="25dp"
                                android:layout_weight="1"
                                android:text="@string/tenant" />
                        </RadioGroup>

                    </LinearLayout>
                </TableRow>

            </TableLayout>

