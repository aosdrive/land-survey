<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cv_floor_partition_1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="0dp"
    app:cardElevation="0dp"
    app:cardBackgroundColor="@color/white"
    app:cardPreventCornerOverlap="false"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/mainLinearLayoutCompact"
        android:layout_width="match_parent"
        android:layout_marginBottom="@dimen/margin_4dp"
        android:layout_height="wrap_content"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:orientation="vertical">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/album_card_form_bg"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/priorityTView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="@string/partition"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/img_delete_partition"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@android:drawable/ic_delete"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:contentDescription="@string/delete_icon" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/white" />

            <TableLayout
                android:id="@+id/tlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/margin_7dp"
                android:layout_marginVertical="2dp">

                <TableRow
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:text="@string/asterisk"
                        android:textColor="@color/textCompulsoryColor"
                        android:textSize="@dimen/edittext_title_size" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/landuse_type"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/edittext_title_size" />

                </TableRow>

                <TableRow>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginVertical="@dimen/margin_4dp"
                        android:layout_weight="1"
                        android:background="@drawable/textfield_area_bg"
                        android:padding="@dimen/margin_4dp">

                        <RadioGroup
                            android:id="@+id/rg_landuse_type"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RadioButton
                                android:layout_width="match_parent"
                                android:layout_height="25dp"
                                android:text="@string/residential" />

                            <RadioButton
                                android:layout_width="match_parent"
                                android:layout_height="25dp"
                                android:text="@string/commercial" />
                        </RadioGroup>
                    </LinearLayout>
                </TableRow>

                <TableRow android:id="@+id/tr_commercial_activity_type_caption"
                    android:visibility="visible"
                    android:layout_marginTop="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:text="@string/asterisk"
                        android:textColor="@color/textCompulsoryColor"
                        android:textSize="@dimen/edittext_title_size" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/commercial_activity_type"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/edittext_title_size" />
                </TableRow>

                <TableRow android:id="@+id/tr_commercial_activity_type"
                    android:visibility="visible"
                    android:layout_marginTop="5dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="@dimen/edittext_height"
                        android:layout_weight="1"
                        android:background="@drawable/textfield_area_bg">

<!--                        <androidx.appcompat.widget.AppCompatEditText-->
<!--                            android:id="@+id/et_commercial_activity_type"-->
<!--                            android:layout_width="match_parent"-->
<!--                            android:layout_height="match_parent"-->
<!--                            android:layout_gravity="center"-->
<!--                            android:layout_margin="5dp"-->
<!--                            android:background="@android:color/transparent"-->
